FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0; 

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    walls
    {
        type            zeroGradient;
    }
    walls_m1
    {
        type            zeroGradient;
    }
    walls_m2
    {
        type            zeroGradient;
    }
    walls_m3
    {
        type            zeroGradient;
    }
    walls_m4
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


